library(ggplot2)
# create example data
group1 <- rnorm(100, mean = 5, sd = 1)
group2 <- rnorm(100, mean = 7, sd = 2)
group3 <- rnorm(100, mean = 10, sd = 3)
data <- data.frame(group=rep(c("group1","group2","group3"), each=100), value=c(group1, group2, group3))
# create boxplot
ggplot(data, aes(x=group, y=value)) +
geom_boxplot()
# create boxplot with all data points
ggplot(data, aes(x=group, y=value)) +
geom_boxplot() +
geom_jitter(alpha = 0.3)
# create boxplot with all data points
# create boxplot with jittered points and narrow width
ggplot(data, aes(x=group, y=value)) +
geom_boxplot() +
geom_jitter(alpha = 0.3, width = 0.2)
# create boxplot with all data points
# create boxplot with jittered points and narrow width
ggplot(data, aes(x=group, y=value,fill=group)) +
geom_boxplot() +
geom_jitter(alpha = 0.3, width = 0.2) +
scale_fill_manual(values=c("group1"="rgb(127, 201, 127)", "group2"="rgb(190, 174, 212)", "group3"="rgb(253, 192, 134)"))
